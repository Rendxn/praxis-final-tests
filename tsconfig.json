{
  "compilerOptions": {
    "target": "es6",
    "sourceMap": true,
    "outDir": "dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "baseUrl": ".",
    "plugins": [
      {
        "transform": "ts-auto-mock/transformer",
        "cacheBetweenTests": false
      }
    ]
  }
}
